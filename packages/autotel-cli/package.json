{
  "name": "autotel-cli",
  "version": "0.1.1",
  "description": "CLI for autotel - setup wizard, diagnostics, and incremental features",
  "type": "module",
  "bin": {
    "autotel": "./dist/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run --config vitest.unit.config.ts",
    "test:watch": "vitest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@inquirer/prompts": "^8.2.0",
    "chalk": "^5.6.2",
    "commander": "^14.0.2",
    "glob": "^13.0.0",
    "ora": "^9.1.0",
    "semver": "^7.7.3",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^25.0.10",
    "@types/semver": "^7.7.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-unicorn": "^62.0.0",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "opentelemetry",
    "tracing",
    "observability",
    "cli",
    "autotel"
  ],
  "license": "MIT"
}
