{
  "name": "autotel-edge",
  "version": "3.9.1",
  "description": "Vendor-agnostic OpenTelemetry for edge runtimes - foundation for Cloudflare, Vercel, Netlify, Deno",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./sampling": {
      "types": "./dist/sampling.d.ts",
      "import": "./dist/sampling.js"
    },
    "./events": {
      "types": "./dist/events.d.ts",
      "import": "./dist/events.js"
    },
    "./logger": {
      "types": "./dist/logger.d.ts",
      "import": "./dist/logger.js"
    },
    "./testing": {
      "types": "./dist/testing.d.ts",
      "import": "./dist/testing.js"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "npx eslint src/**/*.ts",
    "lint:fix": "npx eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rimraf dist"
  },
  "keywords": [
    "opentelemetry",
    "otel",
    "edge",
    "serverless",
    "observability",
    "tracing",
    "vendor-agnostic",
    "cloudflare",
    "vercel",
    "netlify",
    "deno"
  ],
  "author": "Jag Reehal <jag@jagreehal.com> (https://jagreehal.com)",
  "license": "MIT",
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/core": "^2.5.0",
    "@opentelemetry/otlp-exporter-base": "^0.211.0",
    "@opentelemetry/otlp-transformer": "^0.211.0",
    "@opentelemetry/resources": "^2.5.0",
    "@opentelemetry/sdk-trace-base": "^2.5.0",
    "@opentelemetry/semantic-conventions": "^1.39.0"
  },
  "devDependencies": {
    "@opentelemetry/context-async-hooks": "^2.5.0",
    "@types/node": "^25.2.3",
    "rimraf": "^6.1.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "vitest-mock-extended": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jagreehal/autotel",
    "directory": "packages/autotel-edge"
  },
  "bugs": {
    "url": "https://github.com/jagreehal/autotel/issues"
  },
  "homepage": "https://github.com/jagreehal/autotel/tree/main/packages/autotel-edge#readme"
}
